{
  "name": "mi-angular-color-picker",
  "version": "0.2.0",
  "description": "The color picker by formly for AngularJS 1",
  "main": "dist/mi-angular-color-picker.js",
  "scripts": {
    "prebuild:js": "npm run jscs && npm run jshint",
    "build": "npm run build:js && npm run build:js-min",
    "build:js": "webpack --config webpack.config.js",
    "build:js-min": "webpack --config webpack.min.config.js",
    "jscs": "jscs ./src ./test",
    "jshint": "jshint ./src ./test --reporter node_modules/jshint-stylish/index.js",
    "test": "node_modules/karma/bin/karma start karma.config.js"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krimovish/mi-angular-color-picker.git"
  },
  "keywords": [
    "formly",
    "angular"
  ],
  "author": "krimovish",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/krimovish/mi-angular-color-picker/issues"
  },
  "homepage": "https://github.com/krimovish/mi-angular-color-picker#readme",
  "dependencies": {
    "angular-bootstrap-colorpicker": "^3.0.19"
  },
  "devDependencies": {
    "angular": "^1.4.9",
    "angular-mocks": "^1.4.9",
    "istanbul-instrumenter-loader": "^0.2.0",
    "jasmine-core": "^2.3.4",
    "jscs": "^3.0.0",
    "jshint": "^2.8.0",
    "jshint-stylish": "^2.1.0",
    "karma": "^0.13.6",
    "karma-coverage": "^1.0.0",
    "karma-coveralls": "^1.1.2",
    "karma-jasmine": "^1.0.0",
    "karma-junit-reporter": "^1.0.0",
    "karma-ng-html2js-preprocessor": "^1.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "phantomjs-prebuilt": "^2.1.4",
    "webpack": "^1.12.9"
  }
}
